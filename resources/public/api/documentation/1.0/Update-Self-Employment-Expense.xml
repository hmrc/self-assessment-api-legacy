<endpoint>
    <name>Update Self Employment Expense</name>
    <description>This endpoint modifies specific self-employment expense</description>
    <section id="resource">
        <title>Resource</title>
        <resource>PUT /self-assessment/{utr}/self-employments/{selfEmploymentsId}/expenses/{selfEmploymentExpenseId}</resource>
    </section>
    <section id="path-parameters">
        <title>Path Parameters</title>
        <table>
            <headings>
                <cell>Name</cell>
                <cell>Type</cell>
                <cell>Example</cell>
                <cell>Description</cell>
            </headings>
            <rows>
                <row>
                    <cell>
                        <code>utr</code>
                    </cell>
                    <cell>
                        <code>String</code>
                    </cell>
                    <cell>
                        <code>2234567890</code>
                    </cell>
                    <cell>The Self Assessment UTR of the customer</cell>
                </row>
                <row>
                    <cell>
                        <code>selfEmploymentsId</code>
                    </cell>
                    <cell>
                        <code>String</code>
                    </cell>
                    <cell>
                        <code>1234567</code>
                    </cell>
                    <cell>The Self Employment id</cell>
                </row>
                <row>
                    <cell>
                        <code>selfEmploymentExpenseId</code>
                    </cell>
                    <cell>
                        <code>String</code>
                    </cell>
                    <cell>
                        <code>2349587</code>
                    </cell>
                    <cell>The Self Employment Expense id</cell>
                </row>
            </rows>
        </table>
    </section>
    <section id="request-headers">
        <title>Request Headers</title>
        <table>
            <headings>
                <cell>Name</cell>
                <cell>Value</cell>
            </headings>
            <rows>
                <row>
                    <cell>
                        <code>Accept</code>
                    </cell>
                    <cell>
                        <code>application/vnd.hmrc.1.0+json</code>
                    </cell>
                </row>
            </rows>
        </table>
    </section>
    <section id="authorisation">
        <title>Authorisation</title>
        <authorisation>
            <type>USER</type>
            <scope>read:self-assessment</scope>
        </authorisation>
    </section>
    <section id="sample-request">
        <json>
       <![CDATA[{
        "taxYear": "2016-17",
        "category": "TravelCosts",
        "amount": "7777.00"
       }]]>
        </json>
    </section>
    <section id="sample-response">
        <title>Response</title>
        <httpStatus>200 (OK)</httpStatus>
        <json>
<![CDATA[{
    "_links": {
        "self": { "href": "/self-assessment/1097172564/self-employments/1234/expenses/5728b53c4800005100d2d98a" }
    }
}]]>
        </json>
    </section>
</endpoint>
