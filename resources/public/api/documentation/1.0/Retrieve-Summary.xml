<endpoint>
    <name>Retrieve Summary</name>
    <description>This endpoint returns a specific summary</description>
    <section id="resource">
        <title>Resource</title>
        <resource>GET /self-assessment/{utr}/{taxYear}/{source}/{sourceId}/{summary}/{summaryId}</resource>
    </section>
    <section id="path-parameters">
        <title>Path Parameters</title>
        <table>
            <headings>
                <cell>Name</cell>
                <cell>Type</cell>
                <cell>Example</cell>
                <cell>Description</cell>
            </headings>
            <rows>
                <row>
                    <cell>
                        <code>utr</code>
                    </cell>
                    <cell>
                        <code>String</code>
                    </cell>
                    <cell>
                        <code>2234567890</code>
                    </cell>
                    <cell>The Self Assessment UTR of the customer</cell>
                </row>
                <row>
                    <cell>
                        <code>taxYear</code>
                    </cell>
                    <cell>
                        <code>String</code>
                    </cell>
                    <cell>
                        <code>2016-17</code>
                    </cell>
                    <cell>The tax year the data applies to</cell>
                </row>
                <row>
                    <cell>
                        <code>source</code>
                    </cell>
                    <cell>
                        <code>Enum</code>
                    </cell>
                    <cell>
                        <code>self-employments</code>
                    </cell>
                    <cell>The type of source</cell>
                </row>
                <row>
                    <cell>
                        <code>sourceId</code>
                    </cell>
                    <cell>
                        <code>String</code>
                    </cell>
                    <cell>
                        <code>5728b53c4800005100d2d32d</code>
                    </cell>
                    <cell>The source id</cell>
                </row>
                <row>
                    <cell>
                        <code>summary</code>
                    </cell>
                    <cell>
                        <code>Enum</code>
                    </cell>
                    <cell>
                        <code>incomes, expenses</code>
                    </cell>
                    <cell>The type of summary</cell>
                </row>
                <row>
                    <cell><code>summaryId</code></cell>
                    <cell><code>String</code></cell>
                    <cell><code>5728b53c4800005100d2d98a</code></cell>
                    <cell>The summary id</cell>
                </row>
            </rows>
        </table>
    </section>
    <section id="request-headers">
        <title>Request Headers</title>
        <table>
            <headings>
                <cell>Name</cell>
                <cell>Value</cell>
            </headings>
            <rows>
                <row>
                    <cell>
                        <code>Accept</code>
                    </cell>
                    <cell>
                        <code>application/vnd.hmrc.1.0+json</code>
                    </cell>
                </row>
            </rows>
        </table>
    </section>
    <section id="authorisation">
        <title>Authorisation</title>
        <authorisation>
            <type>USER</type>
            <scope>read:self-assessment</scope>
        </authorisation>
    </section>
    <section id="sample-response-expenses">
        <title>Expenses Response</title>
        <httpStatus>200 (OK)</httpStatus>
        <json>
<![CDATA[{
  "_links": {
    "self": {
      "href": "/self-assessment/2234567890/2016-17/self-employments/5728b53c4800005100d2d32d/expenses/5728b53c4800005100d2d98a"
    }
  },
  "id": "5728b53c4800005100d2d98a",
  "type": "CISPayments",
  "amount": 1000.45
}]]>
        </json>
    </section>
    <section id="sample-response-incomes">
        <title>Incomes Response</title>
        <httpStatus>200 (OK)</httpStatus>
        <json>
<![CDATA[{
   "_links":{
      "self":{
         "href":"/self-assessment/2234567890/2016-17/self-employments/5728b53c4800005100d2d32d/incomes/5728b53c4800005100d2d98a"
      }
   },
   "id":"5728b53c4800005100d2d98a",
   "type":"Turnover",
   "amount": 60000.00
}]]>
        </json>
    </section>
</endpoint>
