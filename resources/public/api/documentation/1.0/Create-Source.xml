<endpoint>
    <name>Create Source of Income</name>
    <description>This endpoint creates a source of income</description>
    <section id="resource">
        <title>Resource</title>
        <resource>POST /self-assessment/{utr}/{taxYear}/{source}</resource>
    </section>
    <section id="path-parameters">
        <title>Path Parameters</title>
        <table>
            <headings>
                <cell>Name</cell>
                <cell>Type</cell>
                <cell>Example</cell>
                <cell>Description</cell>
            </headings>
            <rows>
                <row>
                    <cell>
                        <code>utr</code>
                    </cell>
                    <cell>
                        <code>String</code>
                    </cell>
                    <cell>
                        <code>2234567890</code>
                    </cell>
                    <cell>The Self Assessment UTR of the customer</cell>
                </row>
                <row>
                    <cell>
                        <code>taxYear</code>
                    </cell>
                    <cell>
                        <code>String</code>
                    </cell>
                    <cell>
                        <code>2016-17</code>
                    </cell>
                    <cell>The tax year the data applies to</cell>
                </row>
                <row>
                    <cell>
                        <code>source</code>
                    </cell>
                    <cell>
                        <code>Enum</code>
                    </cell>
                    <cell>
                        <code>self-employments</code>
                    </cell>
                    <cell>The type of source to create</cell>
                </row>
            </rows>
        </table>
    </section>
    <section id="request-headers">
        <title>Request Headers</title>
        <table>
            <headings>
                <cell>Name</cell>
                <cell>Value</cell>
            </headings>
            <rows>
                <row>
                    <cell>
                        <code>Accept</code>
                    </cell>
                    <cell>
                        <code>application/vnd.hmrc.1.0+json</code>
                    </cell>
                </row>
                <row>
                    <cell>
                        <code>Content-Type</code>
                    </cell>
                    <cell>
                        <code>application/json</code>
                    </cell>
                </row>
            </rows>
        </table>
    </section>
    <section id="authorisation">
        <title>Authorisation</title>
        <authorisation>
            <type>USER</type>
            <scope>write:self-assessment</scope>
        </authorisation>
    </section>
    <section id="sample-request">
        <title>Self-Employment Request</title>
        <resource>POST /self-assessment/2234567890/2016-17/self-employments</resource>
        <json>
<![CDATA[{
    "name": "Awesome Plumbers",
    "commencementDate": "2016-04-06"
}]]>
        </json>
    </section>
    <section id="sample-response">
        <title>Sample Response</title>
        <httpStatus>201 (CREATED)</httpStatus>
        <json>
<![CDATA[{
    "_links": {
       "self": { "href": "/self-assessment/2234567890/2016-17/self-employments/5728b53c4800005100d2d32d"},
       "incomes": { "href": "/self-assessment/2234567890/2016-17/self-employments/5728b53c4800005100d2d32d/incomes"},
       "expenses": { "href": "/self-assessment/2234567890/2016-17/self-employments/5728b53c4800005100d2d32d/expenses"}
    }
}]]>
        </json>
    </section>
</endpoint>
