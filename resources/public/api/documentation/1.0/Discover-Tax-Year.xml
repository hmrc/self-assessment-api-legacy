<endpoint>
    <name>Discover Tax Year</name>
    <description>This endpoint provides links to the self assessment entry points for the specified tax year</description>
    <section id="resource">
        <title>Resource</title>
        <resource>GET /self-assessment/{utr}/{taxYear}</resource>
    </section>
    <section id="path-parameters">
        <title>Path Parameters</title>
        <table>
            <headings>
                <cell>Name</cell>
                <cell>Type</cell>
                <cell>Example</cell>
                <cell>Description</cell>
            </headings>
            <rows>
                <row>
                    <cell><code>utr</code></cell>
                    <cell><code>String</code></cell>
                    <cell><code>2234567890</code></cell>
                    <cell>The Self Assessment UTR of the customer</cell>
                </row>
                <row>
                    <cell><code>taxYear</code></cell>
                    <cell><code>String</code></cell>
                    <cell><code>2016-17</code></cell>
                    <cell>The tax year the data applies to</cell>
                </row>
            </rows>
        </table>
    </section>
    <section id="request-headers">
        <title>Request Headers</title>
        <table>
            <headings>
                <cell>Name</cell>
                <cell>Value</cell>
            </headings>
            <rows>
                <row>
                    <cell>
                        <code>Accept</code>
                    </cell>
                    <cell>
                        <code>application/vnd.hmrc.1.0+json</code>
                    </cell>
                </row>
            </rows>
        </table>
    </section>
    <section id="authorisation">
        <title>Authorisation</title>
        <authorisation>
            <type>USER</type>
            <scope>read:self-assessment</scope>
        </authorisation>
    </section>
    <section id="sample-response">
        <title>Response</title>
        <httpStatus>200 (OK)</httpStatus>
        <json>
<![CDATA[{
   "_links":{
      "self":{
         "href":"/self-assessment/2234567890/2016-17"
      },
      "self-employments":{
         "href":"/self-assessment/2234567890/2016-17/self-employments"
      },
      "liabilities":{
         "href":"/self-assessment/2234567890/2016-17/liabilities"
      }
   }
}
]]>
        </json>
    </section>
</endpoint>
